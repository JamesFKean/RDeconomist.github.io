{ "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

  "description": "Tik Tok game",

  "title": {
    "text": "Putting a price on TikTok",
    "subtitle":["Percentage of UChicago students that would retain TikTok.",""],
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },

  "height": 300,
  "width": 300,

  "data": {
    "url": "https://raw.githubusercontent.com/RDeconomist/RDeconomist.github.io/main/charts/e4e/ch1_GDP/TikTok_valuationGame.csv",
    "format":{"type":"csv"}},

  "layer":[


  {"mark": {
    "type":"point",
    "size":50,
    "color":"rgb(128,0,0)",
    "opacity":1},
  
  "encoding": {

    "x": {
      "field": "Keep_pct", 
      "type": "quantitative",
      "title":null,
      "axis":{
        "format":"%",
        "grid": false}
      },

    "y": {
      "field": "Dollars", 
      "type": "quantitative",
      "title":null,
      "axis":{
        "format":"$.0f",
        "grid": false}
      }
   }
  },

  {"mark": {
    "type": "rule", 
    "color": "darkgray", 
    "size": 1,
    "strokeDash":[6,3]},

    "encoding": {
      "x": {"datum": 0.5}
      }
  }

   ]
}